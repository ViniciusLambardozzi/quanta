WIDTH=32;
DEPTH=1024;

ADDRESS_RADIX=BIN;
DATA_RADIX=BIN;

CONTENT BEGIN
	-- Function call
	00000000000  :   00000001000000000000000000000001; -- li $0, 1
	00000000001  :   00000001000010000000000000000100; -- li $1, 4
	00000000010  :   00000001000100000000000000000110; -- li $2, 110
	00000000011  :   00000001000110000000000000001000; -- li $3, 1000

	00000000100  :   00010000000100000000000000000000; -- call $2
	00000000101  :   00001011000110000000000000000000; -- j $3


	00000000110  :   00000100000010000000000000000000; -- add $1, $0
	00000000111  :   00001011111110000000000000000000; -- j $31

	00000001000  :   00001011000110000000000000000000; -- j $3

	[00000001001..01111111111]  :   00000000000000000000000000000000;
END;